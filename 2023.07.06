#include <iostream>
#include <string>
using namespace std;

void Student(int studentNum, string& GoodStudentScore, int& maxScore)
{
  for(int i = 0; i < studentNum; i++)
    {
       string name;
       int score;
       
       cin << name;
       cin << score;

       if(score > maxScore)
         {
           maxScore = score;
           GoodstudentScore = name;
          }
}
int main()
{
  Student(studentNum, GoodStudentScore, maxScore);
  
    
   cout << "1st Student Name : " << GoodStudentScore << '\n';
   cout << maxScore << '\n';

   return 0;
}









