#include <iostream>
#include <string>
using namespace std;

int main() {

	string str;
	int checknum = 0;
	
	cin >> str;

	for (int i =0 ; i < str.size() ; i++)
	{
		if (str[i] == 'a' || str[i] == 'e' || str[i] == 'i' || str[i] == 'o' || str[i] == 'u')
		{
			checknum++;
		}
	}

	cout << checknum;
	
	return 0;
}

