//9번
#include <iostream>
using namespace std;

int sumof(int a, int b)
{
	int sum = 0;
	for (int i = a; i <= b; i++)
	{
		sum += i;
	}
	return sum;
}

int main()
{
	int a, b;
	
	cin >> a;
	cin >> b;

	int result = sumof(a, b);
	cout << result << '\n';
}
