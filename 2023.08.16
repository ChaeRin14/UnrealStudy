#include <iostream>
#include <algorithm>
using namespace std;
int A[4],B[2] = {0,};
int main() {
    for (int i = 0; i < 4; i++){
        cin >> A[i];
    }
    sort(A, A + 4);
    
    for (int i = 0; i < 2; i++){
        cin >> B[i];
    }
    sort(B, B + 2);
    
    cout << A[1] + A[2] + A[3] + B[1]<<'\n';
}
